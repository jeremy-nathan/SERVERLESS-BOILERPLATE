{
  "name": "api-serverless",
  "version": "1.0.0",
  "description": "Serverless Node.js Typescript Project",
  "main": "apiLambda.js",
  "engines": {
    "node": ">=14.18.1"
  },
  "private": true,
  "scripts": {
    "env": "sync-dotenv --env .env.stg --samples \".env*\"",
    "db:init": "npx ts-node src/aws/dynamodb/database.spec.ts",
    "db:admin": "npx dynamodb-admin",
    "debug": "node --inspect --lazy node_modules/serverless/bin/serverless offline start --stage=stg --noAuth --noTimeout --localEnvironment",
    "start": "npx sls offline start --allowCache --noAuth",
    "start-stg": "NODE_ENV=staging npm run start -- -s stg",
    "start-stg-ngrok": "npm run start-stg -- --tunnel=true",
    "invoke-local": "npx sls invoke local -f $npm_package_config_functionName -t Event --no-build -s stg",
    "lint": "npx eslint --ext .ts src",
    "format": "prettier --write ./src",
    "build-prod": "NODE_ENV=production npx sls package -s prod -p deploy",
    "build-stg": "NODE_ENV=staging npx sls package -s stg -p deploy",
    "init:prod": "NODE_ENV=production SLS_DEBUG=* npx sls create-cert --stage prod && NODE_ENV=production SLS_DEBUG=* npx sls create_domain --stage prod",
    "init:stg": "NODE_ENV=staging SLS_DEBUG=* npx sls create-cert --stage stg && NODE_ENV=staging SLS_DEBUG=* npx sls create_domain --stage stg",
    "sls-deploy-prod": "NODE_ENV=production npx sls deploy -s prod",
    "sls-deploy-stg": "NODE_ENV=staging npx sls deploy -s stg",
    "sls-deploy-api-prod": "NODE_ENV=production npx sls deploy function -f apiLambda -s prod",
    "sls-deploy-api-stg": "NODE_ENV=staging npx sls deploy function -f apiLambda -s stg",
    "sls-update-api-prod": "npm run sls-deploy-api-prod -- --update-config",
    "sls-update-api-stg": "npm run sls-deploy-api-stg -- --update-config"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@vendia/serverless-express": "^4.10.1",
